{
  "name": "electron-updater-example",
  "version": "0.5.0",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "publish": "build",
    "run": "electron ."
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.43.3"
  },
  "dependencies": {
    "arch": "^2.1.0",
    "crypto": "^1.0.1",
    "electron-log": "^1.3.0",
    "electron-updater": "^2.18.2"
  },
  "build": {
    "appId": "com.github.iffy.electronupdaterexample",
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:8000/api/v1/updates/"
      }
    ],
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    }
  }
}
